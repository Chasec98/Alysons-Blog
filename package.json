{
  "name": "vue-material-kit",
  "version": "1.2.2",
  "private": true,
  "description": "Vue Material Kit",
  "author": "Creative Tim https://www.creative-tim.com/",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service lint && vue-cli-service build && firebase deploy",
    "dev": "npm run serve",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev"
  },
  "dependencies": {
    "core-js": "3.5.0",
    "firebase": "^7.17.1",
    "nouislider": "14.1.0",
    "v-tooltip": "2.0.2",
    "vue": "2.6.10",
    "vue-clickaway": "2.2.2",
    "vue-lazyload": "1.3.3",
    "vue-material": "1.0.0-beta-11",
    "vue-router": "3.1.3",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.1",
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-plugin-router": "4.1.1",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "4.1.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.7.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "6.0.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "prettier": "1.19.1",
    "sass": "1.23.7",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "2.6.10",
    "vuetify-loader": "^1.3.0"
  }
}
